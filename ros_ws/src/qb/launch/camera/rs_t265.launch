<!--
Important Notice: For wheeled robots, odometer input is a requirement for robust
and accurate tracking. The relevant APIs will be added to librealsense and
ROS/realsense in upcoming releases. Currently, the API is available in the
https://github.com/IntelRealSense/librealsense/blob/master/third-party/libtm/libtm/include/TrackingDevice.h#L508-L515.
-->
<launch>
  <!-- Generic arguments -->
  <!-- ############################################################### -->
  <arg name="initial_reset"             default="false"/>
  <arg name="reconnect_timeout"         default="6.0"/>

  <!-- T265 arguments -->
  <!-- ############################################################### -->
  <arg name="serial_no_camera1"         default="905312110732"/>
  <arg name="usb_port_id_camera1"       default=""/>
  <arg name="device_type_camera1"       default="t265"/>
  <arg name="json_file_path"            default="$(find qb)/config/t265_wheel_odom.json"/>
  <arg name="camera1"                   default="t265"/>
  <arg name="tf_prefix_camera1"         default="$(arg camera1)"/>
  <arg name="enable_sync"               default="false"/>
  <arg name="fisheye_width"             default="-1"/> 
  <arg name="fisheye_height"            default="-1"/>
  <arg name="enable_fisheye1"           default="false"/>
  <arg name="enable_fisheye2"           default="false"/>
  <arg name="fisheye_fps"               default="-1"/>
  <arg name="gyro_fps"                  default="-1"/>
  <arg name="accel_fps"                 default="-1"/>
  <arg name="enable_gyro"               default="true"/>
  <arg name="enable_accel"              default="true"/>
  <arg name="enable_pose"               default="true"/>
  <arg name="linear_accel_cov"          default="0.01"/>
  <arg name="unite_imu_method"          default=""/>
  <arg name="base_frame_id"             default="$(arg camera1)_link"/>
  <arg name="pose_frame_id"             default="$(arg camera1)_pose"/>
  <arg name="odom_frame_id"             default="$(arg camera1)_odom"/>
  <arg name="tf_publish_rate"           default="200"/>
  <arg name="publish_tf"                default="false"/>
  <arg name="publish_odom_tf"           default="false"/>
  <!-- If the parameter below is non-empty, the T265 node will attempt to load a saved map from the given path -->
  <arg name="localization_map_filepath" default="/home/whoobee/qb/ros_ws/src/qb/config/t265_saved_map_01.bin"/>

  <!-- D435 arguments -->
  <!-- ############################################################### -->
  <arg name="serial_no_camera2"         default="829212072176"/>
  <arg name="usb_port_id_camera2"       default=""/>
  <arg name="device_type_camera2"       default="d4.5"/>
  <arg name="camera2"                   default="d435"/>
  <arg name="tf_prefix_camera2"         default="$(arg camera2)"/>
  <arg name="color_width"               default="640"/>
  <arg name="color_height"              default="480"/>
  <arg name="depth_width"               default="640"/>
  <arg name="depth_height"              default="480"/>
  <arg name="clip_distance"             default="-2"/>

  <group ns="$(arg camera1)">
    <include file="$(find realsense2_camera)/launch/includes/nodelet.launch.xml">
      <arg name="tf_prefix"                value="$(arg tf_prefix_camera1)"/>
      <arg name="serial_no"                value="$(arg serial_no_camera1)"/>
      <arg name="usb_port_id"              value="$(arg usb_port_id_camera1)"/>
      <arg name="device_type"              value="$(arg device_type_camera1)"/>
      <arg name="json_file_path"           value="$(arg json_file_path)"/>

      <arg name="enable_sync"              value="$(arg enable_sync)"/>

      <arg name="fisheye_width"            value="$(arg fisheye_width)"/>
      <arg name="fisheye_height"           value="$(arg fisheye_height)"/>
      <arg name="enable_fisheye1"          value="$(arg enable_fisheye1)"/>
      <arg name="enable_fisheye2"          value="$(arg enable_fisheye2)"/>

      <arg name="fisheye_fps"              value="$(arg fisheye_fps)"/>
      <arg name="gyro_fps"                 value="$(arg gyro_fps)"/>
      <arg name="accel_fps"                value="$(arg accel_fps)"/>
      <arg name="enable_gyro"              value="$(arg enable_gyro)"/>
      <arg name="enable_accel"             value="$(arg enable_accel)"/>
      <arg name="enable_pose"              value="$(arg enable_pose)"/>

      <arg name="linear_accel_cov"         value="$(arg linear_accel_cov)"/>
      <arg name="initial_reset"            value="$(arg initial_reset)"/>
      <arg name="reconnect_timeout"        value="$(arg reconnect_timeout)"/>
      <arg name="unite_imu_method"         value="$(arg unite_imu_method)"/>
      <arg name="base_frame_id"            value="$(arg base_frame_id)"/>
      <arg name="pose_frame_id"            value="$(arg pose_frame_id)"/> 
      <arg name="odom_frame_id"            value="$(arg odom_frame_id)"/> 
      <arg name="tf_publish_rate"          value="$(arg tf_publish_rate)"/> 
      <arg name="publish_tf"               value="$(arg publish_tf)"/>
      <arg name="publish_odom_tf"          value="$(arg publish_odom_tf)"/>

      <arg name="localization_map_filepath" value="$(arg localization_map_filepath)"/>
    </include>
  </group>

  <group ns="$(arg camera2)">
    <include file="$(find realsense2_camera)/launch/includes/nodelet.launch.xml">
      <arg name="device_type"           value="$(arg device_type_camera2)"/>
      <arg name="serial_no"             value="$(arg serial_no_camera2)"/>
      <arg name="usb_port_id"           value="$(arg usb_port_id_camera2)"/>
      <arg name="tf_prefix"		          value="$(arg tf_prefix_camera2)"/>
      <arg name="initial_reset"         value="$(arg initial_reset)"/>
      <arg name="reconnect_timeout"     value="$(arg reconnect_timeout)"/>
      <arg name="align_depth"           value="true"/>
      <arg name="filters"               value="pointcloud"/>
      <arg name="color_width"           value="$(arg color_width)"/>
      <arg name="color_height"          value="$(arg color_height)"/>
      <arg name="depth_width"           value="$(arg depth_width)"/>
      <arg name="depth_height"          value="$(arg depth_height)"/>
      <arg name="clip_distance"         value="$(arg clip_distance)"/>
    </include>
  </group>
  
</launch>
